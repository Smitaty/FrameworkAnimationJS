	<programs>
		<program assigned_to="rect_center">
			<click />
			<center />
		</program>
		<program assigned_to="rect_blink">
			<click />
			<blink
					times="6"
					delay="20" />
		</program>
		<program assigned_to="rect_centerx">
			<click />
			<centerx />
		</program>
		<program assigned_to="rect_centery">
			<click />
			<centery />
		</program>
		<program assigned_to="rect_click">
			<click />
			<setproperty
					object="rect_click"
					property="text"
					value="clicked" />
		</program>
		<program assigned_to="rect_down">
			<click />
			<down
					y="100"
					dy="1" />
		</program>
		<program assigned_to="rect_goto">
			<click />
			<setproperty
					object="rect_goto"
					property="text"
					value="Test@label" />
		</program>
		<program assigned_to="rect_label">
			<click />
			<label value="label" />
			<down
					y="200"
					dy="10" />
			<up
					y="200"
					dy="10" />
			<goto value="label" />
			<trigger
					object="rect_goto"
					value="goto" />
		</program>
		<program assigned_to="rect_left">
			<click />
			<left
					x="100"
					dx="10" />
		</program>
		<program assigned_to="rect_moveto">
			<click />
			<moveto
					x="100"
					y="100"
					dx="5"
					dy="5" />
		</program>
		<program assigned_to="rect_right">
			<click />
			<right
					x="100"
					dx="5" />
		</program>
		<program assigned_to="rect_sleep">
			<click />
			<down
					y="200"
					dy="10" />
			<setproperty
					object="rect_sleep"
					property="text"
					value="Sleep 2s" />
			<sleep value="2000" />
			<setproperty
					object="rect_sleep"
					property="text"
					value="End" />
		</program>
		<program assigned_to="rect_state">
			<click />
			<setproperty
					object="rect_state"
					property="text"
					value="Déprécié" />
		</program>
		<program assigned_to="rect_stop">
			<click />
			<stop />
		</program>
		<program assigned_to="rect_trigger">
			<click />
			<trigger
					object="rect_wait"
					value="trigger" />
		</program>
		<program assigned_to="rect_up">
			<click />
			<up y="100" dy="1" />
		</program>
		<program assigned_to="rect_wait">
			<click />
			<setproperty
					object="rect_wait"
					property="text"
					value="Attend@trigger" />
			<wait value="trigger" />
			<setproperty
					object="rect_wait"
					property="text"
					value="Trigger@recu" />
		</program>
