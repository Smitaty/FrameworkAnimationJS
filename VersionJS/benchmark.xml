<animation>
	<init>
    	<start_button present="false" />
	</init>

	<background>
		#FFB600
	</background>
	
	<objects>
		<object_text text="test avec gros padding" font="Arial,18,bold" x="800" y="60" visible="true" padding="50" background_transparent="false">a_copier</object_text>
		<object_copy idcopy="a_copier" text="copie du texte avec gros padding" y="150">jl_copy</object_copy>

		<object_grid 
			visible="true" 
			layer="4" 
			background_transparent="false" 
			background_color="0,255,0" 
			border_transparency="false" 
			border_color="20,200,50" 
			border_size="10" 
			lines="3" 
			columns="8" 
			line_height="100" 
			column_width="40" 
			x="350" 
			y="150">LA GRILLE</object_grid>
		<object_grid 
			visible="true" 
			layer="2" 
			background_transparent="false" 
			background_color="100,55,20" 
			border_transparency="false" 
			border_color="20,100,50" 
			lines="3" 
			columns="10" 
			line_height="30" 
			column_width="40" 
			x="420" 
			y="270">test</object_grid>
		
		<object_circle visible="true" background_transparent="false" border_color="0,0,5" background_color="90,80,100" border_transparency="false" radius="70" x="800" y="500">cercle</object_circle>
		<object_ellipse visible="true" background_transparent="false" border_color="50,16,98" background_color="151,11,210" border_transparency="false" border_size="3" width="150" height="100" x="1000" y="500">ellipse</object_ellipse>
		<object_text 
			text="cliquer le cercle et l'ellipse@dans le mÃªme temps" 
			border_size="5" 
			border_transparency="false" 
			border_color="255,0,0" 
			font="Arial,14" 
			x="800" 
			y="700" 
			visible="true" 
			background_transparent="false" 
			width="170">tests deplacement et clics</object_text>

		<object_landmark 
			x="100" y="100"
			visible="true"
			background_transparent="false"
			height="500" width="500"
			scale_x="100" scale_y="100"
			unit_x="patate" unit_y="cm"
			border_color="255,0,0"
			background_color="255,255,0"
			border_transparency="false"> test
		</object_landmark>

		<object_polygon
			x="200"
			y="100"
			coord_x="10,10,0,15,30,20,20,10"
			coord_y=" 0,10,10,20,10,10,0,0"
			border_color="255,0,0"
			background_color="0,128,0"
			background_transparent="false"
			border_transparency="false"
			visible="true"
		>Polygone</object_polygon>

		<object_table 
			x="20" 
			y="20" 
			visible="true"
			background_transparent="false" 
			background_color="100,250,250"
			border_transparency="false"
			border_color="30,30,30"
			values="   | C1 | C2 | C3 | C4 $
					L1 | 10 | 20 | 30 $ 
					L2 | 40 | 50 | 60 | 70 $ 
					L3 | 80 | 90"
			font="Arial,14"
			color="20,255,20"
			line_height="30"
			column_width="60"
			has_header_columns="false"
			has_header_lines="true"
			header_background_color="255,0,0"
			header_color="0,0,255"
			header_font="Courier,24,italic"
			halignment="center"
			valignment="center">Table</object_table>
	</objects>

	<programs>
		<program assigned_to="cercle">
      		<click />
			<moveto x="50" y="100" dx="20" dy="20" delay="5" />
			<blink times="5" delay="5" />
		</program>
		<program assigned_to="ellipse">
      		<click />
			<moveto x="100" y="200" dx="4" dy="1" delay="0" />
			<blink times="5" delay="5" />
		</program>
	</programs>

</animation>
