<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE animation SYSTEM "animation.dtd">
<animation>
	<init>
    	<start_button present="false" />
	</init>

	<background>
		#00A6D6
	</background>
	
	<objects>
		<object_text text="test avec gros padding" round="10,20,30,40" font="Arial,18,bold" x="800" y="60" visible="true" padding="30,40,30,50" background_transparent="false">a_copier</object_text>
		<object_copy idcopy="a_copier" text="copie du texte avec gros padding" y="150">jl_copy</object_copy>

		<object_grid 
			visible="true" 
			layer="4" 
			background_transparent="false" 
			background_color="0,255,0" 
			border_transparency="false" 
			border_color="20,200,50" 
			border_size="10" 
			lines="3" 
			columns="8" 
			line_height="100" 
			column_width="40" 
			x="350" 
			y="150">LA GRILLE</object_grid>
		<object_grid 
			visible="true" 
			layer="2" 
			background_transparent="false" 
			background_color="100,55,20" 
			border_transparency="false" 
			border_color="20,100,50" 
			lines="3" 
			columns="10" 
			line_height="30" 
			column_width="40" 
			x="420" 
			y="270">test</object_grid>
		
		<object_circle visible="true" background_transparent="false" border_color="0,0,5" background_color="90,80,100" border_transparency="false" radius="70" x="800" y="500">cercle</object_circle>
		<object_ellipse visible="true" background_transparent="false" border_color="50,16,98" background_color="151,11,210" border_transparency="false" border_size="3" width="150" height="100" x="1000" y="500">ellipse</object_ellipse>
		<object_text 
			text="cliquer le cercle et l'ellipse@dans le mÃªme temps" 
			border_size="5" 
			border_transparency="false" 
			border_color="255,0,0" 
			font="Arial,14" 
			x="800" 
			y="700"
			round="50"
			visible="true" 
			background_transparent="false" 
			halignment="center"
			valignment="center"
			width="170">tests deplacement et clics</object_text>

		<object_graph 
			x="600" y="600"
			visible="true"
			background_transparent="false"
			height="100" width="-500"
			scale_x="1" scale_y="1"
			max_x ="10" max_y="5"
			min_x ="0" min_y="0"
			unit_x="X" unit_y="Y"
			draw_point="true"
			border_color="255,0,0"
			background_color="255,0,0"
			border_transparency="false"
			function="Math.sin(x)*2"> width neg height pos
		</object_graph>

		<object_graph 
			x="100" y="800"
			visible="true"
			background_transparent="false"
			height="100" width="500"
			scale_x="1" scale_y="1"
			max_x ="15" max_y="0"
			min_x ="0" min_y="-5"
			unit_x="X" unit_y="Y"
			draw_point="true"
			border_color="255,0,0"
			background_color="255,0,0"
			border_transparency="false"
			function="-x*0.3"> width pos height pos
		</object_graph>

		<object_graph 
			x="100" y="850"
			visible="true"
			background_transparent="false"
			height="-100" width="500"
			scale_x="1" scale_y="1"
			max_x ="15" max_y="5"
			min_x ="-5" min_y="-1"
			unit_x="X" unit_y="Y"
			draw_point="true"
			border_color="255,0,0"
			background_color="255,0,0"
			border_transparency="false"
			function="Math.sqrt(x)"> width pos height neg
		</object_graph>

		<object_graph 
			x="1200" y="850"
			visible="true"
			background_transparent="false"
			height="-100" width="-500"
			scale_x="1" scale_y="1"
			max_x ="15" max_y="2"
			min_x ="10" min_y="-5"
			unit_x="X" unit_y="Y"
			draw_point="true"
			border_color="255,0,0"
			background_color="255,0,0"
			border_transparency="false"
			function="-Math.sqrt(x)"> width neg height neg
		</object_graph>

		<object_polygon
			x="400"
			y="100"
			coord_x="10,10,0,15,30,20,20,10"
			coord_y=" 0,10,10,20,10,10,0,0"
			border_color="255,0,0"
			background_color="0,128,0"
			background_transparent="false"
			border_transparency="false"
			visible="true">Polygone</object_polygon>

		<object_table 
			x="0" 
			y="50" 
			layer="5"
			visible="true"
			background_transparent="false" 
			background_color="210,180,222"
			border_transparency="false"
			border_color="30,30,30"
			values="   | C1 | C2 | C3 | C4 $
					L1 | 10 | 20 | 30 $ 
					L2 | 40 | 50 | 60 | 70 $ 
					L3 | 80 | 90"
			font="Arial,14"
			color="169,50,38"
			line_height="30"
			column_width="80"
			has_header_columns="true"
			has_header_lines="true"
			header_background_color="133,193,233"
			header_color="0,0,255"
			header_font="Courier,24,italic"
			header_column_width="60"
			header_line_height="50"
			halignment="center"
			valignment="center">Table</object_table>

		<object_arrow
			x="100"
			y="300"
			visible="true"
			background_transparent="false" 
			background_color="210,180,222"
			border_transparency="false"
			border_color="255,0,0"
			width_line="50"
			height_line="5"
			width_triangle="15"
			height_triangle="15"
			rotation="-90">Arrow</object_arrow>

	</objects>

	<programs>
		<program assigned_to="cercle">
      		<click />
			<moveto x="50" y="100" dx="20" dy="20" delay="5" />
			<blink times="5" delay="5" />
		</program>
		<program assigned_to="ellipse">
      		<click />
			<moveto x="100" y="200" dx="4" dy="1" delay="0" />
			<blink times="5" delay="5" />
		</program>
	</programs>

</animation>
